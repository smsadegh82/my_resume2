{
	"extends": ["standard", "prettier", "prettier/standard"],
	"plugins": ["import", "prettier", "standard"],
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"rules": {
		"no-unused-vars": "off",
		"space-before-function-paren": 0,
		"new-cap": 0,
		"prettier/prettier": 2
	}
}
